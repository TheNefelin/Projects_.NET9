<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    x:Class="MyMauiApp.AppShell"
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:views="clr-namespace:MyMauiApp.Views"
    Title="MyMauiApp">

    <TabBar>
        <Tab Title="Passwords" Icon="lock.png">
            <ShellContent ContentTemplate="{DataTemplate views:PasswordManagerPage}" />
        </Tab>
        <Tab Title="Session" Icon="user.png">
            <ShellContent ContentTemplate="{DataTemplate views:SessionPage}" />
        </Tab>
    </TabBar>

</Shell>
